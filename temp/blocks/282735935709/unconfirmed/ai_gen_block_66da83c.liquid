{% doc %}
  @prompt
    Please code a timeline section for me with a vertical layout, one side for text and the other for media (which could be an image or video), with interchangeable options. Optimize the code for fast page load times. The text column should have five characters at the top and a description at the bottom. Also, add a line in the middle to mark the timestamps. I have about 20 timestamps.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-timeline-{{ ai_gen_id }} {
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-timeline-heading-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
  }

  .ai-timeline-container-{{ ai_gen_id }} {
    position: relative;
    padding: 0;
  }

  .ai-timeline-line-{{ ai_gen_id }} {
    position: absolute;
    left: 50%;
    top: 0;
    bottom: 0;
    width: {{ block.settings.line_width }}px;
    background-color: {{ block.settings.line_color }};
    transform: translateX(-50%);
    z-index: 1;
  }

  .ai-timeline-item-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr {{ block.settings.line_width }}px 1fr;
    gap: {{ block.settings.item_gap }}px;
    margin-bottom: {{ block.settings.item_spacing }}px;
    position: relative;
  }

  .ai-timeline-item-{{ ai_gen_id }}:last-child {
    margin-bottom: 0;
  }

  .ai-timeline-content-{{ ai_gen_id }} {
    padding: {{ block.settings.content_padding }}px;
    background-color: {{ block.settings.content_bg_color }};
    border-radius: {{ block.settings.border_radius }}px;
    position: relative;
    z-index: 2;
  }

  .ai-timeline-content-left-{{ ai_gen_id }} {
    grid-column: 1;
    text-align: right;
  }

  .ai-timeline-content-right-{{ ai_gen_id }} {
    grid-column: 3;
    text-align: left;
  }

  .ai-timeline-marker-{{ ai_gen_id }} {
    grid-column: 2;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    z-index: 3;
  }

  .ai-timeline-dot-{{ ai_gen_id }} {
    width: {{ block.settings.dot_size }}px;
    height: {{ block.settings.dot_size }}px;
    background-color: {{ block.settings.dot_color }};
    border: {{ block.settings.dot_border_width }}px solid {{ block.settings.dot_border_color }};
    border-radius: 50%;
    box-shadow: 0 0 0 4px {{ block.settings.background_color }};
  }

  .ai-timeline-timestamp-{{ ai_gen_id }} {
    font-size: {{ block.settings.timestamp_size }}px;
    font-weight: 700;
    color: {{ block.settings.timestamp_color }};
    margin-bottom: 12px;
    letter-spacing: 0.5px;
  }

  .ai-timeline-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_size }}px;
    line-height: 1.6;
    color: {{ block.settings.description_color }};
  }

  .ai-timeline-media-{{ ai_gen_id }} {
    width: 100%;
    border-radius: {{ block.settings.media_border_radius }}px;
    overflow: hidden;
    margin-top: 16px;
  }

  .ai-timeline-media-{{ ai_gen_id }} img,
  .ai-timeline-media-{{ ai_gen_id }} video {
    width: 100%;
    height: auto;
    display: block;
  }

  .ai-timeline-media-placeholder-{{ ai_gen_id }} {
    width: 100%;
    aspect-ratio: 16 / 9;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: {{ block.settings.media_border_radius }}px;
  }

  .ai-timeline-media-placeholder-{{ ai_gen_id }} svg {
    width: 60px;
    height: 60px;
    opacity: 0.3;
  }

  @media screen and (max-width: 768px) {
    .ai-timeline-line-{{ ai_gen_id }} {
      left: 20px;
    }

    .ai-timeline-item-{{ ai_gen_id }} {
      grid-template-columns: 40px 1fr;
      gap: 20px;
    }

    .ai-timeline-content-{{ ai_gen_id }} {
      grid-column: 2;
      text-align: left;
    }

    .ai-timeline-content-left-{{ ai_gen_id }},
    .ai-timeline-content-right-{{ ai_gen_id }} {
      grid-column: 2;
      text-align: left;
    }

    .ai-timeline-marker-{{ ai_gen_id }} {
      grid-column: 1;
    }

    .ai-timeline-heading-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.8);
      margin-bottom: 40px;
    }
  }
{% endstyle %}

<div class="ai-timeline-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.heading != blank %}
    <h2 class="ai-timeline-heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
  {% endif %}

  <div class="ai-timeline-container-{{ ai_gen_id }}">
    <div class="ai-timeline-line-{{ ai_gen_id }}"></div>

    {% for i in (1..20) %}
      {% liquid
        assign timestamp_key = 'timestamp_' | append: i
        assign description_key = 'description_' | append: i
        assign media_type_key = 'media_type_' | append: i
        assign image_key = 'image_' | append: i
        assign video_key = 'video_' | append: i

        assign timestamp = block.settings[timestamp_key]
        assign description = block.settings[description_key]
        assign media_type = block.settings[media_type_key]
        assign image = block.settings[image_key]
        assign video = block.settings[video_key]

        assign is_left = i | modulo: 2
      %}

      {% if timestamp != blank or description != blank %}
        <div class="ai-timeline-item-{{ ai_gen_id }}">
          {% if is_left == 1 %}
            <div class="ai-timeline-content-{{ ai_gen_id }} ai-timeline-content-left-{{ ai_gen_id }}">
              {% if timestamp != blank %}
                <div class="ai-timeline-timestamp-{{ ai_gen_id }}">{{ timestamp }}</div>
              {% endif %}
              {% if description != blank %}
                <div class="ai-timeline-description-{{ ai_gen_id }}">{{ description }}</div>
              {% endif %}
            </div>

            <div class="ai-timeline-marker-{{ ai_gen_id }}">
              <div class="ai-timeline-dot-{{ ai_gen_id }}"></div>
            </div>

            <div class="ai-timeline-content-{{ ai_gen_id }} ai-timeline-content-right-{{ ai_gen_id }}">
              {% if media_type == 'image' and image != blank %}
                <div class="ai-timeline-media-{{ ai_gen_id }}">
                  <img
                    src="{{ image | image_url: width: 800 }}"
                    alt="{{ image.alt | escape }}"
                    width="{{ image.width }}"
                    height="{{ image.height }}"
                    loading="lazy"
                  >
                </div>
              {% elsif media_type == 'video' and video != blank %}
                <div class="ai-timeline-media-{{ ai_gen_id }}">
                  {{ video | video_tag: autoplay: false, loop: false, controls: true, muted: false }}
                </div>
              {% endif %}
            </div>
          {% else %}
            <div class="ai-timeline-content-{{ ai_gen_id }} ai-timeline-content-left-{{ ai_gen_id }}">
              {% if media_type == 'image' and image != blank %}
                <div class="ai-timeline-media-{{ ai_gen_id }}">
                  <img
                    src="{{ image | image_url: width: 800 }}"
                    alt="{{ image.alt | escape }}"
                    width="{{ image.width }}"
                    height="{{ image.height }}"
                    loading="lazy"
                  >
                </div>
              {% elsif media_type == 'video' and video != blank %}
                <div class="ai-timeline-media-{{ ai_gen_id }}">
                  {{ video | video_tag: autoplay: false, loop: false, controls: true, muted: false }}
                </div>
              {% endif %}
            </div>

            <div class="ai-timeline-marker-{{ ai_gen_id }}">
              <div class="ai-timeline-dot-{{ ai_gen_id }}"></div>
            </div>

            <div class="ai-timeline-content-{{ ai_gen_id }} ai-timeline-content-right-{{ ai_gen_id }}">
              {% if timestamp != blank %}
                <div class="ai-timeline-timestamp-{{ ai_gen_id }}">{{ timestamp }}</div>
              {% endif %}
              {% if description != blank %}
                <div class="ai-timeline-description-{{ ai_gen_id }}">{{ description }}</div>
              {% endif %}
            </div>
          {% endif %}
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>

